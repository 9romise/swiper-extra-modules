{
  "name": "swiper-extra-modules",
  "version": "0.1.1",
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "description": "some additional modules for swiperjs",
  "author": "Vida Xie<https://github.com/9romise>",
  "homepage": "https://github.com/9romise/swiper-extra-modules#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/swiper-extra-modules.git"
  },
  "scripts": {
    "rollup": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "build": "esno scripts/build.ts",
    "watch": "esno scripts/build.ts --watch",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "playground": "cd playground && vite"
  },
  "keywords": [
    "swiper",
    "module"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/9romise/swiper-extra-modules/issues"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.2",
    "bumpp": "^9.4.0",
    "consola": "^3.2.3",
    "esno": "^4.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.7"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "swiper": "^11.1.0"
  }
}
