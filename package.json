{
  "name": "swiper-extra-modules",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@8.15.4",
  "description": "some additional modules for swiperjs",
  "author": "Vida Xie<https://github.com/9romise>",
  "license": "MIT",
  "homepage": "https://github.com/9romise/swiper-extra-modules#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/swiper-extra-modules.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/swiper-extra-modules/issues"
  },
  "keywords": [
    "swiper",
    "module"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "rollup": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "build": "tsx scripts/build.ts",
    "watch": "tsx scripts/build.ts --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "playground": "cd playground && vite"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "swiper": "^11.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.3",
    "@vida0905/eslint-config": "^0.1.2",
    "bumpp": "^9.4.0",
    "changelogithub": "^0.13.5",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.8"
  },
  "pre-commit": [
    "lint"
  ]
}
